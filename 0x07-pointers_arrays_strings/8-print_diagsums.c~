#include "main.h"
#include <stdio.h>

/**
 * print_diagsums - print additions of the two diagonals.
 * @a: source matrix
 * @size: size NxN of matrix
 *
 * Return: chessboard.
 */

void print_diagsums(int *a, int size);
{
	int i, j, sum1 = 0, sum2 = 0;

	for (i = 0; *(a + i); i++)
	{
		sum += a[i][i]
	}
}