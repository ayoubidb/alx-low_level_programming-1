#include "main.h"

/**
 * reset_to_98 - a function that takes a pointer to an int as parameter
 *               and updates the value it points to to 98.
 *@c :  a pointer to an int
 *
 * return: void
 */

void reset_to_98(int *n)
{
	*n = 98;
}
